{
  "name": "emoji-chatbot",
  "version": "1.0.0",
  "description": "A chatbot that translates words to emojis.",
  "dependencies": {
    "emojilib": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.2.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "compile": "tsc",
    "compile:watch": "tsc --watch",
    "format": "prettier --check \"./**/*.{ts,tsx,json,yml,md}\"",
    "test": "react-scripts test --env=jsdom",
    "coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/moji-translate/*"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "gh-pages": "^3.1.0",
    "prettier": "^2.0.5",
    "react-scripts": "^4.0.3",
    "typescript": "^4.2.4"
  },
  "repository": "https://github.com/joelgeorgev/emoji-chatbot",
  "keywords": [
    "typescript",
    "React",
    "reactjs",
    "redux",
    "react-redux",
    "redux-saga",
    "emoji",
    "chatbot",
    "translate"
  ],
  "author": "Joel George V",
  "homepage": "https://emojibot.joelgeorgev.dev",
  "license": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
